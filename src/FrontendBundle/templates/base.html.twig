<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <title>{% block title %}My Bundle{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/frontend/css/bootstrap.css') }}">
    {% endblock %}

    {% block javascripts %}
        <script src="{{ asset('bundles/frontend/js/bootstrap.js') }}"></script>
        <script src="{{ asset('bundles/frontend/js/jquery.js') }}"></script>
    {% endblock %}
</head>
<body>
    {% block body %}{% endblock %} 
</body>
</html>
